{
  "name": "@deltares/webgl-streamline-visualizer",
  "description": "Particle-based flow field visualiser based on WebGL",
  "license": "MIT",
  "private": false,
  "version": "4.3.1",
  "type": "module",
  "files": [
    "dist"
  ],
  "module": "./dist/webgl-streamline-visualizer.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/webgl-streamline-visualizer.js"
    }
  },
  "homepage": "https://deltares.github.io/webgl-streamline-visualizer/",
  "scripts": {
    "dev": "vite --config vite-examples.config.ts",
    "build": "tsc && vite build --config vite-library.config.ts",
    "build-examples": "tsc && vite build --config vite-examples.config.ts",
    "lint": "eslint \"./src/**/*.ts\" \"./examples/**/*.ts\"",
    "preview": "vite preview",
    "test": "vitest",
    "test-once": "vitest run"
  },
  "devDependencies": {
    "@deltares/fews-wms-requests": "^2.0.0",
    "@eslint/js": "^9.25.1",
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.14.1",
    "@vitest/browser": "^3.1.2",
    "eslint": "^9.25.1",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.2.6",
    "playwright": "^1.52.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.31.0",
    "vite": "^6.3.2",
    "vite-plugin-glsl": "^1.4.1",
    "vitest": "^3.1.2"
  },
  "dependencies": {
    "geotiff": "^2.1.3",
    "lodash-es": "^4.17.21",
    "maplibre-gl": "^5.4.0"
  },
  "volta": {
    "node": "22.14.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/deltares/webgl-streamline-visualizer.git"
  },
  "bugs": {
    "url": "https://github.com/deltares/webgl-streamline-visualizer/issues"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
